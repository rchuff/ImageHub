<%- include("partials/header"); %>
<h1 class="title user-login-header">Welcome
  <%= firstName %>
  <%= lastName %>
</h1>
<img class="user-profile" src="/images/meditation.jpg" alt="meditation">

<% images.forEach((image) => { %>
<% let num = images.indexOf(image); %>


<% if (num == 0 || num % 2 ==0) { %>
<div class="row">
  <div class="col-md-6">
    <img class="user-image <%= num %>" src="<%= image %>" alt="nature_pic">
    <p class="user-quote">
      <%= quotes[num] %>
    </p>
  </div>
  <%  } else { %>
  <div class="col-md-6">
    <img class="user-image <%= num %>" src="<%= image %>" alt="nature_pic">
    <p class="user-quote">
      <%= quotes[num] %>
    </p>
  </div>
</div>
<% } %>
<% }); %>




<%- include("partials/footer") %>
